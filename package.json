{
  "name": "@infactory/infactory-mcp",
  "version": "0.0.2",
  "type": "module",
  "description": "Infactory MCP package",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "mcp:dev": "tsx src/index.ts",
    "mcp:dev:watch": "tsx --watch src/index.ts",
    "build": "tsc && chmod 755 dist/index.js",
    "build:watch": "tsc --watch",
    "test": "npm run build && node test/server.test.mjs",
    "test:watch": "concurrently \"npm run build:watch\" \"nodemon --watch dist --watch test test/server.test.mjs\""
  },
  "keywords": [],
  "author": "Sean Kruzel <sean@infactory.ai>",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.17.30",
    "@types/react": "^19.1.0",
    "concurrently": "^9.1.2",
    "dotenv": "^16.4.7",
    "esbuild": "^0.25.2",
    "nodemon": "^3.1.9",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2",
    "zod": "^3.24.2"
  },
  "dependencies": {
    "@infactory/infactory-ts": "^0.5.3",
    "@modelcontextprotocol/sdk": "^1.8.0"
  }
}
